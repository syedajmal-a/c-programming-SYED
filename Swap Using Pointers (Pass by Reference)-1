#include <stdio.h>

// Swap function using pointers (pass by reference)
void swap(int *x, int *y) {
    int temp = *x;
    *x = *y;
    *y = temp;
}

int main() {
    int a, b;
    scanf("%d %d", &a, &b);

    swap(&a, &b);  // Pass addresses of a and b

    printf("After Swap: %d %d\n", a, b);

    return 0;
}
