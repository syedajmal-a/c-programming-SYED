#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int N, M;
    scanf("%d %d", &N, &M);

    int a[N][M];

    // Read matrix
    for(int i = 0; i < N; i++) {
        for(int j = 0; j < M; j++) {
            scanf("%d", &a[i][j]);
        }
    }

    // Find max in each column
    for(int j = 0; j < M; j++) {
        int max = a[0][j];
        for(int i = 1; i < N; i++) {
            if(a[i][j] > max) {
                max = a[i][j];
            }
        }
        printf("%d\n", max);
    }

    return 0;
}
